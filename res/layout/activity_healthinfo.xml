<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_healthinfo"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android1:background="@drawable/common_background"
    tools:context=".LoginActivity" >

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_header"
        android1:layout_width="fill_parent"
        android1:layout_height="95px"
        android1:layout_alignParentLeft="true"
        android1:layout_alignParentTop="true"
        android1:background="#00a086" >

        <ImageButton
            android1:id="@+id/imgbtn_healthinfo_back"
            android1:layout_width="55px"
            android1:layout_height="55px"
            android1:layout_centerVertical="true"
            android1:layout_marginLeft="20px"
            android1:background="@android:color/transparent"
            android1:scaleType="fitCenter"
            android1:src="@drawable/btn_back" />

        <TextView
            android1:id="@+id/lbl_healthinfo_title"
            android1:layout_width="400px"
            android1:layout_height="match_parent"
            android1:layout_centerHorizontal="true"
            android1:background="@android:color/transparent"
            android1:gravity="center"
            android1:text="@string/activity_healthinfo_title"
            android1:textColor="@android:color/white"
            android1:textSize="@dimen/title_fnt_size" />
    </RelativeLayout>

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_bminote"
        android1:layout_width="426px"
        android1:layout_height="52px"
        android1:layout_below="@id/rl_healthinfo_header"
        android1:layout_centerHorizontal="true"
        android1:layout_marginTop="54px" >

        <TextView
            android1:id="@+id/lbl_healthinfo_bmi"
            android1:layout_width="wrap_content"
            android1:layout_height="fill_parent"
            android1:layout_alignParentLeft="true"
            android1:gravity="left|center_vertical"
            android1:text="BMI =23.9 健康體重"
            android1:textColor="@color/common_normal_text"
            android1:textSize="34px" />

        <ImageView
            android1:id="@+id/img_healthinfo_bmihelp"
            android1:layout_width="52px"
            android1:layout_height="52px"
            android1:layout_alignParentRight="true"
            android1:src="@drawable/activity_healthinfo_questionmark" />

    </RelativeLayout>

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_bmrnote"
        android1:layout_width="426px"
        android1:layout_height="52px"
        android1:layout_below="@id/rl_healthinfo_bminote"
        android1:layout_centerHorizontal="true"
        android1:layout_marginTop="20px" >

        <TextView
            android1:id="@+id/lbl_healthinfo_bmr"
            android1:layout_width="wrap_content"
            android1:layout_height="fill_parent"
            android1:layout_alignParentLeft="true"
            android1:gravity="left|center_vertical"
            android1:text="BMR=XXX.XX"
            android1:textColor="@color/color_black"
            android1:textSize="34px" />

        <ImageView
            android1:id="@+id/img_healthinfo_bmrhelp"
            android1:layout_width="52px"
            android1:layout_height="52px"
            android1:layout_alignParentRight="true"
            android1:src="@drawable/activity_healthinfo_questionmark" />
    </RelativeLayout>

    <TextView
        android1:id="@+id/lbl_healthinfo_note"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignParentLeft="false"
        android1:layout_below="@id/rl_healthinfo_bmrnote"
        android1:layout_centerHorizontal="true"
        android1:layout_marginTop="40px"
        android1:textColor="#6e6e6e"
        android1:textSize="32px" />

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_targetnote"
        android1:layout_width="96px"
        android1:layout_height="74px"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@id/lbl_healthinfo_note"
        android1:layout_marginLeft="47px"
        android1:layout_marginTop="54px"
        android1:background="@drawable/activity_healthinfo_noteback1" >

        <TextView
            android1:id="@+id/lbl_healthinfo_targetcaption"
            android1:layout_width="fill_parent"
            android1:layout_height="wrap_content"
            android1:layout_alignParentTop="true"
            android1:layout_marginTop="8px"
            android1:gravity="center_vertical|center_horizontal"
            android1:text="@string/activity_healthinfo_standardnote"
            android1:textColor="#bbbbbb"
            android1:textSize="@dimen/small_fnt_size" />

        <TextView
            android1:id="@+id/lbl_healthinfo_targetvalue"
            android1:layout_width="fill_parent"
            android1:layout_height="wrap_content"
            android1:layout_alignParentBottom="true"
            android1:layout_marginBottom="18px"
            android1:gravity="center_vertical|center_horizontal"
            android1:text="57.2kg"
            android1:textColor="#bbbbbb"
            android1:textSize="@dimen/small_fnt_size" />

    </RelativeLayout>

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_linebar"
        android1:layout_width="626px"
        android1:layout_height="27px"
        android1:layout_below="@id/rl_healthinfo_targetnote"
        android1:layout_centerHorizontal="true" >

        <RelativeLayout
            android1:id="@+id/rl_healthinfo_line1"
            android1:layout_width="50px"
            android1:layout_height="fill_parent"
            android1:background="#13e4c2" >

        </RelativeLayout>

        <RelativeLayout
            android1:id="@+id/rl_healthinfo_line2"
            android1:layout_width="50px"
            android1:layout_height="fill_parent"
            android1:layout_toRightOf="@id/rl_healthinfo_line1"
            android1:background="#07b99c" >

        </RelativeLayout>

        <RelativeLayout
            android1:id="@+id/rl_healthinfo_line3"
            android1:layout_width="50px"
            android1:layout_height="fill_parent"
            android1:layout_toRightOf="@id/rl_healthinfo_line2"
            android1:background="#00a086" >

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_curnote"
        android1:layout_width="96px"
        android1:layout_height="74px"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@id/rl_healthinfo_linebar"
        android1:layout_marginLeft="350px"
        android1:background="@drawable/activity_healthinfo_noteback2" >

        <TextView
            android1:id="@+id/lbl_healthinfo_curcaption"
            android1:layout_width="fill_parent"
            android1:layout_height="wrap_content"
            android1:layout_alignParentTop="true"
            android1:layout_marginTop="18px"
            android1:gravity="center_vertical|center_horizontal"
            android1:text="@string/activity_healthinfo_currentnote"
            android1:textColor="@color/common_normal_text"
            android1:textSize="@dimen/small_fnt_size" />

        <TextView
            android1:id="@+id/lbl_healthinfo_curvalue"
            android1:layout_width="fill_parent"
            android1:layout_height="wrap_content"
            android1:layout_alignParentBottom="true"
            android1:layout_marginBottom="8px"
            android1:gravity="center_vertical|center_horizontal"
            android1:text="65.0kg"
            android1:textColor="@color/common_normal_text"
            android1:textSize="@dimen/small_fnt_size" />
    </RelativeLayout>

    <TextView
        android1:id="@+id/lbl_healthinfo_suggestmsg"
        android1:layout_width="580px"
        android1:layout_height="wrap_content"
        android1:layout_below="@id/rl_healthinfo_curnote"
        android1:layout_centerHorizontal="true"
        android1:layout_marginTop="75px"
        android1:textColor="@color/color_black"
        android1:textSize="32px" />

    <Button
        android1:id="@+id/btn_healthinfo_next"
        android1:layout_width="527px"
        android1:layout_height="90px"
        android1:layout_alignParentBottom="true"
        android1:layout_centerHorizontal="true"
        android1:layout_marginBottom="70px"
        android1:background="@drawable/btn_green"
        android1:text="@string/activity_healthinfo_btn_next"
        android1:textColor="@color/color_white"
        android1:textSize="@dimen/button_label_fnt_size" />

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_mask"
        android1:layout_width="fill_parent"
        android1:layout_height="fill_parent" >

    </RelativeLayout>

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_bmrdescription"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_below="@id/rl_healthinfo_header"
        android1:layout_centerHorizontal="true"
        android1:layout_marginTop="190px" >

        <ImageView
            android1:id="@+id/img_healthinfo_triup"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:layout_marginLeft="425px"
            android1:src="@drawable/tri_up_green" />

        <TextView
            android1:id="@+id/lbl_healthinfo_bmrdescription"
            android1:layout_width="500px"
            android1:layout_height="wrap_content"
            android1:layout_below="@id/img_healthinfo_triup"
            android1:background="@drawable/common_green_radius"
            android1:paddingBottom="10px"
            android1:paddingLeft="20px"
            android1:paddingRight="20px"
            android1:paddingTop="10px"
            android1:text="This is BMI"
            android1:textColor="@color/color_white"
            android1:textSize="@dimen/normal_fnt_size" />

    </RelativeLayout>

    <RelativeLayout
        android1:id="@+id/rl_healthinfo_bmidescription"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignParentTop="true"
        android1:layout_centerHorizontal="true"
        android1:layout_marginTop="210px" >

        <ImageView
            android1:id="@+id/ImageView01"
            android1:layout_width="wrap_content"
            android1:layout_height="wrap_content"
            android1:layout_marginLeft="425px"
            android1:src="@drawable/tri_up_green" />

        <TextView
            android1:id="@+id/lbl_healthinfo_bmidescription"
            android1:layout_width="500px"
            android1:layout_height="wrap_content"
            android1:layout_below="@+id/ImageView01"
            android1:background="@drawable/common_green_radius"
            android1:paddingBottom="10px"
            android1:paddingLeft="20px"
            android1:paddingRight="20px"
            android1:paddingTop="10px"
            android1:text="This is BMI"
            android1:textColor="@color/color_white"
            android1:textSize="@dimen/normal_fnt_size" />
    </RelativeLayout>

</RelativeLayout>